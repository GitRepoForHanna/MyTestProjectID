<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite thread-count="1" verbose="10" name="BookingSuite" annotations="JDK" parallel="tests" >
<!--    <suite name="BookingSuite" annotations="JDK" >-->
    <listeners>
        <listener class-name="utils.ScreenShotListener"/>
        <listener class-name="com.epam.reportportal.testng.ReportPortalTestNGListener"/>
    </listeners>
    
    <test name="Flights">
        <classes>
            <class name="booking.flights.FindFlight"/>
        </classes>
    </test>

    <test name="Car">
        <classes>
            <class name="booking.car.CarRent"/>
        </classes>
    </test>
</suite>

